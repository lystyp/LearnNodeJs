<!DOCTYPE html>
<html>
  <head>
    <title>Ordered list page</title>
  </head>
  <body>
    <form name="form1">
      <table border="1">
          <tr>
            <td>ID</td><td>Product</td><td>Quantity</td><td>Price</td>
          </tr>
        
        <% for (var i in product_list) { %>
          <tr>
            <td><%= product_list[i].id%></td>
            <td><%= product_list[i].name%></td>
            <td><%= product_list[i].order_quantity%></td>
            <td><%= product_list[i].order_price%></td>
          </tr>
        <% } %>
      </table>
    </form>
    <a>Total prices : <%= total_price%></a> 
    <br/>
    <br/>
    <a href="/member/updatePage">Update member information</a> 
    <br/>
    <br/>
    <a href="/product">See all products</a> 
    <br/>
    <br/>
    <a href="/member/logout">Logout</a> 
  </body>
</html>

